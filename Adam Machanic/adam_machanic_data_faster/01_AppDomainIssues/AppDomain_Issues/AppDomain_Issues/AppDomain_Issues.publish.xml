    <DeployScriptFileName>AppDomain_Issues.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=.\ss2014;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ScriptDatabaseOptions>False</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <DropIndexesNotInSource>False</DropIndexesNotInSource>
