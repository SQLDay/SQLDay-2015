    <DropConstraintsNotInSource>False</DropConstraintsNotInSource>
    <DisableAndReenableDdlTriggers>False</DisableAndReenableDdlTriggers>
    <DropStatisticsNotInSource>False</DropStatisticsNotInSource>
    <IgnoreAnsiNulls>False</IgnoreAnsiNulls>
    <IgnoreAuthorizer>True</IgnoreAuthorizer>
    <IgnoreColumnCollation>True</IgnoreColumnCollation>
    <IgnoreComments>True</IgnoreComments>
